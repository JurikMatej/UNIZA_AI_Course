            },
            {
                "type"      : "relu"
            },
            {
              "type" : "dense convolution",
              "geometry" : [3, 3, 8]
            },
            {
                "type"      : "relu"
            },
            {
              "type" : "dense convolution",
              "geometry" : [3, 3, 8]
            },
            {
                "type"      : "relu"
            },
            {
              "type" : "dense convolution",
              "geometry" : [3, 3, 8]
            },
            {
                "type"      : "relu"
            },


            {
                "type"      : "dropout"
            },
            {
                "type"      : "output"
            }

        ]
  }
}
